{{#*inline "title"}}
laenorweb2
{{/inline}}

{{#*inline "page"}}
<div class="header">
  <h1>laenorweb2</h1>
  <h2>^o^</h2>
</div>

<div class="pure-g">
  <div class="pure-u-lg-1-5"></div>
  <div class="pure-u-1 pure-u-lg-3-5">
    <div class="pure-g">
      <div class="pure-u-1"><hr/></div>

      {{#each highlights as |tag| ~}}
      <div class="pure-u-1 pure-u-md-1-3">
        <a href="/tags/{{ tag.id }}">
        <div class="box area">
          <div class="maploc"><pre>{{ color_map tag.map }}</pre></div>
          <div class="name">{{ tag.name }}</div>
        </div>
        </a>
      </div>
      {{/each~}}

      <div class="pure-u-1"><hr/></div>

      <div class="pure-u-md-1-3">
        <div class="box">
          <a class="pure-button" href="note/new">New note</a>
        </div>
      </div>
      <div class="pure-u-1 pure-u-md-1-3">
        <div class="box">
          <input type="text" class="filter" placeholder="Filter"></input>
        </div>
      </div>
      <div class="pure-u-md-1-3">
        <div class="box">
          <input type="text" class="filter" placeholder="Search all"></input>
          <button class="pure-button">Search</button>
        </div>
      </div>
    </div>
  </div>
  <div class="pure-u-lg-1-5"></div>

  {{#each taggroups as |tg| ~}}
  <div class="pure-u-1"><div class="box">
    <h3>{{ tg.0.name }}</h3>
  </div></div>
  {{#each tg.1 as |tag| ~}}
  <div class="pure-u-1 pure-u-md-1-3 pure-u-lg-1-5">
    <a href="/tags/{{ tag.id }}">
    <div class="box area">
      <div class="maploc"><pre>{{ color_map tag.map }}</pre></div>
      <div class="name">{{ tag.name }}</div>
    </div>
    </a>
  </div>
  {{/each~}}
  {{/each~}}
</div>

{{/inline}}
{{~> (parent)~}}
